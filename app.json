{
  "expo": {
    "name": "SideBet",
    "slug": "sidebet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1024.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sidebet.app",
      "jsEngine": "jsc",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-512.png",
        "backgroundColor": "#111827"
      },
      "jsEngine": "jsc",
      "package": "com.sidebet.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon-192.png",
      "name": "SideBet",
      "shortName": "SideBet",
      "lang": "en",
      "scope": "/",
      "themeColor": "#111827",
      "backgroundColor": "#111827",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#111827"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-192.png",
          "color": "#111827",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SideBet to access your camera to scan QR codes for quick bet joining."
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon-192.png",
      "color": "#111827",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new SideBet notifications"
    },
    "scheme": "sidebet",
    "developmentClient": {
      "silentLaunch": false
    },
    "extra": {
      "eas": {
        "projectId": "f26fa72b-c85a-4174-90bb-1b14c526ed05"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "bursicd"
  }
}
